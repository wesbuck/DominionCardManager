{
  "name": "dominion-card-manager",
  "description": "Works with DominionCardAPI to manage Dominion cards.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wesbuck/DominionCardManager.git"
  },
  "keywords": [],
  "author": "Wes Buck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wesbuck/DominionCardManager/issues"
  },
  "homepage": "https://github.com/wesbuck/DominionCardManager",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.16.1",
    "express": "^4.17.1",
    "jquery": "^3.6.0",
    "serve-static": "^1.14.1",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-service": "^4.5.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.1.0",
    "node-sass": "^6.0.1",
    "popper.js": "^1.16.1",
    "prettier-eslint": "^13.0.0",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.44.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/base",
      "eslint:recommended"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
